njt_addon_name=njt_http_kafka_module
njt_module_type=HTTP
njt_module_name=$njt_addon_name

njt_module_srcs="$njt_addon_dir/src/njt_http_kafka_module.c"

njt_module_incs="$njt_addon_dir/src \
    $njt_addon_dir/../../auto/lib/librdkafka/include"

njt_module_libs="-L$njt_addon_dir/../../auto/lib/librdkafka/build/src/ -lrdkafka"

have=NJT_HTTP_RDKAFKA . auto/have

. auto/module