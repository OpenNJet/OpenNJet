njt_addon_name=njt_stream_ws_module

if test -n "$njt_stream_proto_server_module_SRCS"; then
    echo "found njt_stream_proto_server_module; looks good!"
else
    echo "error: njt_stream_proto_server_module is required to build $njt_addon_name; please put it before $njt_addon_name!" 1>&2
    exit 1
fi
njt_module_type=STREAM
njt_module_name=$njt_addon_name
njt_module_deps=" \
    $njt_addon_dir/../njet-stream-proto-server-module/src/njt_tcc.h \
    $njt_addon_dir/src/http/api.h \
    $njt_addon_dir/src/http/http_parser.h \
    $njt_addon_dir/src/http/llhttp.h \
    $njt_addon_dir/src/ws/proto_ws.h\
"
njt_module_srcs="\
    $njt_addon_dir/src/njt_stream_ws_module.c \
    $njt_addon_dir/src/http/api.c \
    $njt_addon_dir/src/http/http.c \
    $njt_addon_dir/src/http/http_parser.c \
    $njt_addon_dir/src/http/llhttp.c \
    $njt_addon_dir/src/http/proto_http.c \
    $njt_addon_dir/src/ws/proto_ws.c\
    "
njt_module_incs="$njt_addon_dir/src"
njt_module_libs=""
njt_module_order=""
#have= . auto/have
. auto/module


