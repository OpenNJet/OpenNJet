case "$CC" in

    cl)

cat << END

$0: error:  not allow build use cl

END

    ;;

    bcc32)

cat << END

$0: error:  not allow build use bcc32

END

    ;;

    *)

        cat << END                                            >> $NJT_MAKEFILE
$MODTCC/libtcc.a:
	cd  $MODTCC \\
    && CC_OPT='' CFLAGS='' ./configure --with-selinux  --prefix=. \\
    && \$(MAKE) 
END
    ;;

esac
